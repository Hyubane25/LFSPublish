{
  "hosting": {
    "public": "x64",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "**/*.application",
        "headers": [
          { "key": "Content-Type", "value": "application/x-ms-application" }
        ]
      },
      {
        "source": "**/*.manifest",
        "headers": [
          { "key": "Content-Type", "value": "application/x-ms-manifest" }
        ]
      },
      {
        "source": "**/*.deploy",
        "headers": [
          { "key": "Content-Type", "value": "application/octet-stream" }
        ]
      },
      {
        "source": "**/*.exe",
        "headers": [
          { "key": "Content-Type", "value": "application/x-msdownload" }
        ]
      }
    ]
  }
}
